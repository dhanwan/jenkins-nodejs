pipeline {
    agent any
    enviroment{
        PATH = "/opt/maven/bin:$PATH"
    }
    
    stages {
        stage("Source Code Clone"){
            steps {
                git branch: 'main', url: 'https://github.com/dhanwanit/jenkins-nodejs.git'
            }
        }
        
        stage("Cheking MAven Version"){
            
            steps {
                sh "java --version"
                sh "mvn --version"
            }
        }
    }
}
